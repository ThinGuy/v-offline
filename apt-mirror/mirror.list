############################  config ####################################
#
set base_path    /srv/mirror
set mirror_path  /srv/mirror/archives
set skel_path    /srv/mirror/skel
set var_path     /srv/mirror/var
set defaultarch  amd64
set postmirror_script /srv/mirror/bin/postmirror.sh
set cleanscript /srv/mirror/var/clean.sh
set run_postmirror 1
set nthreads     80
set _tilde 0
#set limit_rate 100m


